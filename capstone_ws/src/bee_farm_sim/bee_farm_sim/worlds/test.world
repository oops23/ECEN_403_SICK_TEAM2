<?xml version="1.0" ?>
<sdf version="1.6">
  <world name="farm_world">

    <!-- Physics -->
    <physics name="default_physics" type="ode">
      <max_step_size>0.001</max_step_size>
      <real_time_factor>1</real_time_factor>
      <real_time_update_rate>1000</real_time_update_rate>
    </physics>

    <!-- Gravity -->
    <gravity>0 0 -9.8</gravity>

    <!-- Atmosphere -->
    <atmosphere type="adiabatic"/>

    <!-- Scene -->
    <scene>
      <ambient>0.4 0.4 0.4 1</ambient>
      <background>0.7 0.7 0.7 1</background>
      <shadows>true</shadows>
    </scene>

    <!-- Ground Plane -->
    <model name='ground_plane'>
      <static>true</static>
      <link name='link'>
        <collision name='collision'>
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>100 100</size>
            </plane>
          </geometry>
          <surface>
            <friction><ode/></friction>
          </surface>
        </collision>
        <visual name='visual'>
          <geometry>
            <plane>
              <normal>0 0 1</normal>
              <size>100 100</size>
            </plane>
          </geometry>
          <material>
            <ambient>0.8 0.8 0.8 1</ambient>
            <diffuse>0.8 0.8 0.8 1</diffuse>
            <specular>0.8 0.8 0.8 1</specular>
          </material>
        </visual>
      </link>
    </model>

    <!-- Grass Patch -->
    <model name="grass_patch">
      <static>true</static>
      <link name="link">
        <visual name="grass_visual">
          <geometry>
            <box>
              <size>15 15 0.1</size>
            </box>
          </geometry>
          <material>
            <ambient>0.1 0.6 0.1 1</ambient>
            <diffuse>0.1 0.6 0.1 1</diffuse>
          </material>
          <pose>0 0 0.05 0 0 0</pose>
        </visual>
        <collision name="grass_collision">
          <geometry>
            <box>
              <size>15 15 0.1</size>
            </box>
          </geometry>
        </collision>
      </link>
    </model>

    <!-- 30 Random Flowers -->
    <!-- flower format: <model name="flowerX"><link><visual stem><visual top></link><pose/></model> -->

    <model name="flower1">
      <static>true</static>
      <link name="link">
        <visual name="stem_visual">
          <geometry><cylinder><radius>0.05</radius><length>3.1</length></cylinder></geometry>
          <material><ambient>0 0.5 0 1</ambient><diffuse>0 0.5 0 1</diffuse></material>
          <pose>0 0 1.55 0 0 0</pose>
        </visual>
        <visual name="top_visual">
          <geometry><sphere><radius>0.1</radius></sphere></geometry>
          <material><ambient>1 0 0 1</ambient><diffuse>1 0 0 1</diffuse></material>
          <pose>0 0 3.1 0 0 0</pose>
        </visual>
        <collision name="stem_collision">
          <geometry><cylinder><radius>0.05</radius><length>3.1</length></cylinder></geometry>
          <pose>0 0 1.55 0 0 0</pose>
        </collision>
        <collision name="top_collision">
          <geometry><sphere><radius>0.1</radius></sphere></geometry>
          <pose>0 0 3.1 0 0 0</pose>
        </collision>
      </link>
      <pose>-6.21 3.47 0 0 0 0</pose>
    </model>

    <model name="flower14">
    <static>true</static>
    <link name="link">
        <!-- Stem -->
        <visual name="stem_visual">
        <geometry><cylinder><radius>0.05</radius><length>3.0</length></cylinder></geometry>
        <material><ambient>0 0.5 0 1</ambient><diffuse>0 0.5 0 1</diffuse></material>
        <pose>0 0 1.5 0 0 0</pose>
        </visual>
        <!-- Top -->
        <visual name="top_visual">
        <geometry><sphere><radius>0.1</radius></sphere></geometry>
        <material><ambient>1 0.5 0 1</ambient><diffuse>1 0.5 0 1</diffuse></material>
        <pose>0 0 3.0 0 0 0</pose>
        </visual>
        <!-- Collisions -->
        <collision name="stem_collision">
        <geometry><cylinder><radius>0.05</radius><length>3.0</length></cylinder></geometry>
        <pose>0 0 1.5 0 0 0</pose>
        </collision>
        <collision name="top_collision">
        <geometry><sphere><radius>0.1</radius></sphere></geometry>
        <pose>0 0 3.0 0 0 0</pose>
        </collision>

        <!-- Bee visual attached to the flower -->
        <visual name="bee_visual">
        <geometry>
            <box><size>0.2 0.2 0.2</size></box>
        </geometry>
        <material>
            <ambient>0 0 0 1</ambient>
            <diffuse>0 0 0 1</diffuse>
        </material>
        <!-- position relative to flower link -->
        <pose>0 0 3.1 0 0 0</pose>
        </visual>
        <collision name="bee_collision">
        <geometry>
            <box><size>0.2 0.2 0.2</size></box>
        </geometry>
        <pose>0 0 3.1 0 0 0</pose>
        </collision>

    </link>
    <!-- World pose -->
    <pose>-5.98816 -4.31248 0 0 0 0</pose>
    </model>


    <!-- Flower 15 -->
    <model name="flower15">
      <static>true</static>
      <link name="link">
        <!-- Stem -->
        <visual name="stem_visual">
          <geometry><cylinder><radius>0.05</radius><length>4.0</length></cylinder></geometry>
          <material><ambient>0 0.5 0 1</ambient><diffuse>0 0.5 0 1</diffuse></material>
          <pose>0 0 2.0 0 0 0</pose>
        </visual>
        <collision name="stem_collision">
          <geometry><cylinder><radius>0.05</radius><length>4.0</length></cylinder></geometry>
          <pose>0 0 2.0 0 0 0</pose>
        </collision>

        <!-- Top -->
        <visual name="top_visual">
          <geometry><sphere><radius>0.1</radius></sphere></geometry>
          <material><ambient>0.5 0 0.5 1</ambient><diffuse>0.5 0 0.5 1</diffuse></material>
          <pose>0 0 4.0 0 0 0</pose>
        </visual>
        <collision name="top_collision">
          <geometry><sphere><radius>0.1</radius></sphere></geometry>
          <pose>0 0 4.0 0 0 0</pose>
        </collision>
      </link>
      <pose>3.77 0.88 0 0 0 0</pose>
    </model>

    <!-- Flower 16 -->
    <model name="flower16">
      <static>true</static>
      <link name="link">
        <!-- Stem -->
        <visual name="stem_visual">
          <geometry><cylinder><radius>0.05</radius><length>3.0</length></cylinder></geometry>
          <material><ambient>0 0.5 0 1</ambient><diffuse>0 0.5 0 1</diffuse></material>
          <pose>0 0 1.5 0 0 0</pose>
        </visual>
        <!-- Top -->
        <visual name="top_visual">
          <geometry><sphere><radius>0.1</radius></sphere></geometry>
          <material><ambient>1 0.5 0 1</ambient><diffuse>1 0.5 0 1</diffuse></material>
          <pose>0 0 3.0 0 0 0</pose>
        </visual>
        <!-- Collisions -->
        <collision name="stem_collision">
          <geometry><cylinder><radius>0.05</radius><length>3.0</length></cylinder></geometry>
          <pose>0 0 1.5 0 0 0</pose>
        </collision>
        <collision name="top_collision">
          <geometry><sphere><radius>0.1</radius></sphere></geometry>
          <pose>0 0 3.0 0 0 0</pose>
        </collision>
        <!-- Bee visual attached to the flower -->
        <visual name="bee_visual">
        <geometry>
            <box><size>0.2 0.2 0.2</size></box>
        </geometry>
        <material>
            <ambient>0 0 0 1</ambient>
            <diffuse>0 0 0 1</diffuse>
        </material>
        <!-- position relative to flower link -->
        <pose>0 0 3.1 0 0 0</pose>
        </visual>
        <collision name="bee_collision">
        <geometry>
            <box><size>0.2 0.2 0.2</size></box>
        </geometry>
        <pose>0 0 3.1 0 0 0</pose>
        </collision>
      </link>
      <!-- World pose -->
      <pose>-0.55 4.11 0 0 0 0</pose>
    </model>


    <!-- Directional Light -->
    <light name='sun' type='directional'>
      <pose>0 0 10 0 0 0</pose>
      <cast_shadows>true</cast_shadows>
      <intensity>1</intensity>
      <direction>-0.5 0.1 -0.9</direction>
      <diffuse>0.8 0.8 0.8 1</diffuse>
      <specular>0.2 0.2 0.2 1</specular>
      <attenuation>
        <range>1000</range>
        <linear>0.01</linear>
        <constant>0.9</constant>
        <quadratic>0.001</quadratic>
      </attenuation>
    </light>

  </world>
</sdf>